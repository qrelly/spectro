<?xml version="1.0" encoding="utf-8" ?>
<article spectro-label="Article" spectro-icon="article.svg">
	<h1 spectro-label="Article title" spectro-icon="h1.svg"></h1>
	<h2 spectro-label="Heading" spectro-icon="h2.svg"></h2>
	<h3 spectro-label="Subheading" spectro-icon="h3.svg"></h3>
	<p spectro-label="Paragraph" spectro-icon="p.svg" spectro-editable="true">
		<b spectro-label="Bold text" spectro-icon="strong.svg"></b>
		<i spectro-label="Italic text" spectro-icon=".svg"></i>
		<a href="//" spectro-label="Link"></a>
	</p>
	<ul spectro-label="List">
		<li spectro-label="List item">
			<h3 spectro-label="Subheading"></h3>
			<p spectro-label="Paragraph" spectro-editable="true">
				<b spectro-label="Bold text"></b>
				<i spectro-label="Italic text"></i>
				<a href="//" spectro-label="Link"></a>
			</p>
		</li>
	</ul>
	<img src="//placehold.it/720x320" alt="" spectro-label="Image" />
</article>